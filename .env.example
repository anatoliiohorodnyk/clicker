# ===== LOGGING =====
# Log level: DEBUG, INFO, WARNING, ERROR
LOG_LEVEL=INFO

# ===== CAPTCHA AI PROVIDER =====
# Choose provider: gemini, cloudflare, or openai
CAPTCHA_PROVIDER=gemini

# ----- Gemini Settings -----
# Get API key from https://aistudio.google.com/app/apikey
GEMINI_API_KEY=your_gemini_api_key_here
GEMINI_MODEL=gemini-2.0-flash

# ----- Cloudflare Workers AI Settings (FREE, recommended) -----
# Uses native Cloudflare API for free vision models
# CAPTCHA_PROVIDER=cloudflare
# OPENAI_API_BASE=https://api.cloudflare.com/client/v4/accounts/{account_id}/ai/v1
# OPENAI_API_KEY=your_cf_api_token
# OPENAI_MODEL=@cf/llava-hf/llava-1.5-7b-hf
# Alternative free model: @cf/unum/uform-gen2-qwen-500m

# ----- OpenAI-compatible Settings -----
# Works with: OpenAI GPT-4, other OpenAI-compatible APIs
# CAPTCHA_PROVIDER=openai
# OPENAI_API_BASE=https://api.openai.com/v1
# OPENAI_API_KEY=sk-...
# OPENAI_MODEL=gpt-4o

# ===== SIMPLEMMO AUTHENTICATION =====
# Option 1 (recommended): Auto-login with email/password
# Bot will automatically obtain API token and session cookies
SIMPLEMMO_EMAIL=your_email@example.com
SIMPLEMMO_PASSWORD=your_password_here

# Option 2: Manual configuration (if auto-login doesn't work)
# Get these from browser DevTools
# SIMPLEMMO_API_TOKEN=your_api_token_here
# SIMPLEMMO_LARAVEL_SESSION=your_laravelsession_cookie_here
# SIMPLEMMO_XSRF_TOKEN=your_xsrf_token_here

# Bot settings
STEP_DELAY_MIN=5
STEP_DELAY_MAX=8
STEPS_PER_SESSION=0

# Break pauses (human-like behavior)
BREAK_INTERVAL_MIN=500
BREAK_INTERVAL_MAX=700
BREAK_DURATION_MIN=300
BREAK_DURATION_MAX=420

# Features (true/false)
AUTO_FIGHT_NPC=true
AUTO_GATHER_MATERIALS=true
ONLY_QUESTS=false
