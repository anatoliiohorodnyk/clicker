# SimpleMMO API Token
# Get it from browser DevTools -> Network tab -> travel request body
SIMPLEMMO_API_TOKEN=your_api_token_here

# Google Gemini API Key for captcha solving
# Get it from https://aistudio.google.com/app/apikey
GEMINI_API_KEY=your_gemini_api_key_here

# ===== AUTHENTICATION (choose one method) =====

# Option 1: Auto-login (recommended) - bot will login automatically
SIMPLEMMO_EMAIL=your_email@example.com
SIMPLEMMO_PASSWORD=your_password_here

# Option 2: Manual session cookies - get from DevTools -> Application -> Cookies
# Leave email/password empty if using this method
# SIMPLEMMO_LARAVEL_SESSION=your_laravelsession_cookie_here
# SIMPLEMMO_XSRF_TOKEN=your_xsrf_token_here

# Bot settings
STEP_DELAY_MIN=3
STEP_DELAY_MAX=8
STEPS_PER_SESSION=100

# Features (true/false)
AUTO_FIGHT_NPC=true
AUTO_GATHER_MATERIALS=true
